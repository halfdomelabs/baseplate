{
  "name": "@prisma-crud/admin",
  "version": "1.0.0",
  "private": true,
  "description": "Web app for todo-with-auth0",
  "license": "UNLICENSED",
  "type": "module",
  "files": [
    "build/**/*"
  ],
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "gql:check": "gql-tada check -c tsconfig.app.json",
    "gql:generate": "gql-tada generate-output -c tsconfig.app.json",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier -w .",
    "preview": "vite preview",
    "test": "vitest run",
    "typecheck": "tsc -b --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/client": "4.0.11",
