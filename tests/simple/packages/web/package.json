    "clsx": "2.1.1",
    "graphql": "16.9.0",
    "loglevel": "1.9.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
    "react-hook-form": "7.53.0",
    "react-hot-toast": "2.4.1",
    "react-icons": "5.5.0",
    "react-router-dom": "6.22.3",
    "react-select": "5.8.0",
    "zod": "3.24.1",
