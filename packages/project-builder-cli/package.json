{
  "name": "@baseplate/project-builder-cli",
  "version": "0.1.0",
  "description": "Full-stack CLI builder using Baseplate generators",
  "main": "dist/index.js",
  "repository": "https://github.com/kingston/baseplate/tree/master/packages/project-builder-cli",
  "author": "Kingston Tam",
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@baseplate/tools": "^0.0.1",
    "@types/ramda": "^0.27.64",
    "@types/yup": "^0.29.13",
    "ts-node-dev": "^1.1.8",
    "tsc-alias": "^1.5.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "start": "yarn build && node dist/index.js",
