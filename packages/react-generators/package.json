{
  "name": "@halfdomelabs/react-generators",
  "version": "0.2.1",
  "description": "React Generators for Baseplate",
  "main": "dist/index.js",
  "repository": "https://github.com/halfdomelabs/baseplate",
  "author": "Half Dome Labs LLC",
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "LICENSE",
    "generator.json",
    "dist/**/*",
    "CHANGELOG"
  ],
  "devDependencies": {
    "@halfdomelabs/tools": "workspace:*",
    "@types/node": "^18.0.0",
    "@types/lodash": "^4.14.191",
    "@types/ramda": "^0.27.64",
    "@types/toposort": "^2.0.3",
    "concurrently": "^7.0.0",
    "del": "^6.0.0",
    "tsc-alias": "1.8.6",
    "typescript": "4.9.5"
  },
  "scripts": {
    "build": "concurrently pnpm:build:*",
    "build:tsc": "tsc && tsc-alias",
    "build:templates": "gulp -f gulpfile.cjs buildTemplates",
    "watch": "concurrently pnpm:watch:*",
    "watch:tsc": "tsc --preserveWatchOutput --watch & tsc-alias --watch",
    "watch:templates": "gulp -f gulpfile.cjs watchTemplates",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier -w ."
  },
  "dependencies": {
    "@halfdomelabs/core-generators": "workspace:*",
    "@halfdomelabs/sync": "workspace:*",
    "@types/resolve": "^1.20.2",
    "gulp": "^4.0.2",
    "inflection": "2.0.1",
    "lodash": "^4.17.21",
    "ramda": "^0.28.0",
    "resolve": "^1.22.2",
    "toposort": "^2.0.2",
    "zod": "^3.20.6"
  },
  "engines": {
    "node": "^18.0.0"
  },
  "volta": {
    "node": "18.16.0"
  },
  "type": "module"
}
