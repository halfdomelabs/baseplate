{
  "name": "@halfdomelabs/react-generators",
  "version": "0.7.4",
  "description": "React Generators for Baseplate",
  "homepage": "https://www.halfdomelabs.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/halfdomelabs/baseplate",
    "directory": "packages/react-generators"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Half Dome Labs LLC",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "LICENSE",
    "generator.json",
    "dist/**/*",
    "CHANGELOG"
  ],
  "scripts": {
    "build": "concurrently pnpm:build:*",
    "build:templates": "cpx \"src/generators/*/*/templates/**\" \"dist/generators\" --clean",
    "build:tsc": "tsc && tsc-alias",
    "clean": "rm -rf ./dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier -w .",
    "watch": "concurrently pnpm:watch:*",
    "watch:templates": "pnpm build:templates --watch",
    "watch:tsc": "tsc --preserveWatchOutput -w",
    "watch:tsc-alias": "tsc-alias -w"
  },
  "dependencies": {
    "@halfdomelabs/core-generators": "workspace:*",
    "@halfdomelabs/sync": "workspace:*",
    "@types/resolve": "^1.20.2",
    "inflection": "2.0.1",
    "lodash": "^4.17.21",
    "prettier-plugin-tailwindcss": "0.5.14",
    "ramda": "^0.28.0",
    "resolve": "^1.22.2",
    "toposort": "^2.0.2",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@halfdomelabs/tools": "workspace:*",
    "@types/lodash": "4.14.196",
    "@types/node": "^20.0.0",
    "@types/ramda": "^0.29.3",
    "@types/toposort": "^2.0.3",
    "concurrently": "8.2.2",
    "cpx2": "^5.0.0",
    "tsc-alias": "1.8.8",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": "^20.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
