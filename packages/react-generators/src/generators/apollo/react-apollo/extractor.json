{
  "name": "apollo/react-apollo",
  "extractors": {
    "ts": {
      "importProviders": [
        "@baseplate-dev/react-generators:graphqlImportsProvider"
      ]
    }
  },
  "templates": {
    "app-apollo-provider": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "generator": "@baseplate-dev/react-generators#apollo/react-apollo",
      "group": "main",
      "importMapProviders": {},
      "pathRootRelativePath": "{src-root}/app/app-apollo-provider.tsx",
      "referencedGeneratorTemplates": ["service"],
      "sourceFile": "src/app/app-apollo-provider.tsx",
      "variables": {
        "TPL_CREATE_ARGS": {},
        "TPL_MEMO_DEPENDENCIES": {},
        "TPL_RENDER_BODY": {}
      }
    },
    "cache": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "generator": "@baseplate-dev/react-generators#apollo/react-apollo",
      "group": "main",
      "importMapProviders": {},
      "pathRootRelativePath": "{src-root}/services/apollo/cache.ts",
      "projectExports": { "createApolloCache": {} },
      "sourceFile": "src/services/apollo/cache.ts",
      "variables": {}
    },
    "graphql": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "generator": "@baseplate-dev/react-generators#apollo/react-apollo",
      "group": "main",
      "importMapProviders": {},
      "pathRootRelativePath": "{src-root}/graphql.ts",
      "projectExports": {
        "FragmentOf": { "isTypeOnly": false, "name": "FragmentOf" },
        "graphql": { "isTypeOnly": false, "name": "graphql" },
        "readFragment": { "isTypeOnly": false, "name": "readFragment" },
        "ResultOf": { "isTypeOnly": false, "name": "ResultOf" },
        "VariablesOf": { "isTypeOnly": false, "name": "VariablesOf" }
      },
      "referencedGeneratorTemplates": ["graphql-env-d"],
      "sourceFile": "src/graphql.ts",
      "variables": {}
    },
    "graphql-config": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "group": "main",
      "importMapProviders": {},
      "pathRootRelativePath": "{package-root}/graphql.config.ts",
      "sourceFile": "package/graphql.config.ts",
      "variables": { "TPL_BACKEND_SCHEMA_PATH": {} }
    },
    "graphql-env-d": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "group": "main",
      "importMapProviders": {},
      "pathRootRelativePath": "{src-root}/graphql-env.d.ts",
      "projectExports": {
        "introspection": { "isTypeOnly": true, "name": "introspection" },
        "introspection_types": {
          "isTypeOnly": true,
          "name": "introspection_types"
        }
      },
      "projectExportsOnly": true,
      "sourceFile": "src/graphql-env.d.ts",
      "variables": {}
    },
    "service": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "generator": "@baseplate-dev/react-generators#apollo/react-apollo",
      "group": "main",
      "importMapProviders": {},
      "pathRootRelativePath": "{src-root}/services/apollo/index.ts",
      "projectExports": { "createApolloClient": {} },
      "referencedGeneratorTemplates": ["cache"],
      "sourceFile": "src/services/apollo/index.ts",
      "variables": {
        "TPL_CREATE_ARGS": {},
        "TPL_LINK_BODIES": {},
        "TPL_LINKS": {}
      }
    },
    "websocket-links": {
      "type": "ts-snippet",
      "sourceFile": "websocket-links.ts"
    }
  }
}
