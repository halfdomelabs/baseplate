{
  "name": "@baseplate-dev/project-builder-web",
  "version": "0.3.2",
  "description": "Web interface for constructing project builder JSON",
  "keywords": [
    "react",
    "project-builder",
    "ui",
    "baseplate",
    "full-stack",
    "typescript",
    "code-generation"
  ],
  "homepage": "https://www.baseplate.dev",
  "repository": "https://github.com/halfdomelabs/baseplate",
  "license": "MPL-2.0",
  "author": "Half Dome Labs LLC",
  "type": "module",
  "imports": {
    "#src/*": "./dist/*"
  },
  "files": [
    "README.md",
    "LICENSE",
    "CHANGELOG",
    "dist/**/*",
    "!dist/**/*.d.ts.map",
    "!dist/**/*.tsbuildinfo"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "clean": "rm -rf ./dist",
    "dev": "vite",
    "dev:serve": "vite --clearScreen false",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier -w .",
    "serve": "vite preview",
    "test": "vitest",
    "typecheck": "tsc -b --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@baseplate-dev/project-builder-lib": "workspace:*",
    "@baseplate-dev/ui-components": "workspace:*",
    "@baseplate-dev/utils": "workspace:*",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.1",
    "@hookform/lenses": "0.8.1",
    "@hookform/resolvers": "5.0.1",
    "@tanstack/react-router": "1.130.8",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.2.0",
    "@trpc/client": "^11.1.2",
    "@trpc/server": "^11.1.2",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "anser": "2.3.2",
    "clsx": "2.1.1",
    "culori": "^4.0.1",
    "es-toolkit": "1.31.0",
    "immer": "10.1.1",
    "inflection": "3.0.0",
    "jsdom": "26.0.0",
    "loglevel": "1.9.1",
    "react": "catalog:",
    "react-dom": "catalog:",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "7.60.0",
    "react-icons": "5.5.0",
    "react-timeago": "8.2.0",
    "semver": "^7.5.4",
    "zod": "catalog:",
    "zustand": "5.0.3"
  },
  "devDependencies": {
    "@baseplate-dev/project-builder-server": "workspace:*",
    "@baseplate-dev/tools": "workspace:*",
    "@originjs/vite-plugin-federation": "1.3.6",
    "@tailwindcss/vite": "catalog:",
    "@tanstack/router-plugin": "1.130.8",
    "@types/culori": "^2.1.1",
    "@types/node": "catalog:",
    "@types/semver": "^7.5.0",
    "@vitejs/plugin-react": "catalog:",
    "chokidar": "4.0.3",
    "eslint": "catalog:",
    "mime": "^4.0.3",
    "prettier": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vite-plugin-svgr": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
