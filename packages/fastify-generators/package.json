{
  "name": "@halfdomelabs/fastify-generators",
  "version": "0.10.9",
  "description": "Fastify generators for Baseplate",
  "homepage": "https://www.halfdomelabs.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/halfdomelabs/baseplate",
    "directory": "packages/fastify-generators"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Half Dome Labs LLC",
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "LICENSE",
    "generator.json",
    "dist/**/*",
    "CHANGELOG"
  ],
  "scripts": {
    "build": "concurrently pnpm:build:*",
    "build:templates": "cpx \"src/generators/*/*/templates/**\" \"dist/generators\" --clean",
    "build:tsc": "tsc && tsc-alias",
    "clean": "rm -rf ./dist",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier -w .",
    "typecheck": "tsc --noEmit",
    "watch": "concurrently pnpm:watch:*",
    "watch:templates": "pnpm build:templates --watch",
    "watch:tsc": "tsc --preserveWatchOutput -w",
    "watch:tsc-alias": "tsc-alias -w"
  },
  "dependencies": {
    "@halfdomelabs/core-generators": "workspace:*",
    "@halfdomelabs/sync": "workspace:*",
    "@prisma/internals": "5.7.1",
    "@types/eslint": "catalog:",
    "change-case": "5.4.4",
    "indent-string": "5.0.0",
    "inflection": "3.0.0",
    "ramda": "^0.28.0",
    "semver": "^7.5.4",
    "ts-morph": "23.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@halfdomelabs/tools": "workspace:*",
    "@types/ramda": "^0.29.3",
    "@types/semver": "^7.5.0",
    "concurrently": "9.0.1",
    "cpx2": "7.0.1",
    "tsc-alias": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": "^20.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
