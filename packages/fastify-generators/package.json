{
  "name": "@halfdomelabs/fastify-generators",
  "version": "0.6.2",
  "description": "Fastify generators for Baseplate",
  "main": "dist/index.js",
  "repository": "https://github.com/halfdomelabs/baseplate",
  "author": "Half Dome Labs LLC",
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "LICENSE",
    "generator.json",
    "dist/**/*",
    "CHANGELOG"
  ],
  "devDependencies": {
    "@halfdomelabs/tools": "workspace:*",
    "@types/ramda": "^0.29.3",
    "@types/semver": "^7.5.0",
    "concurrently": "^8.2.0",
    "cpx2": "^5.0.0",
    "tsc-alias": "1.8.7",
    "typescript": "5.1.6"
  },
  "scripts": {
    "build": "concurrently pnpm:build:*",
    "build:tsc": "tsc && tsc-alias",
    "build:templates": "cpx \"src/generators/*/*/templates/**\" \"dist/generators\" --clean",
    "watch": "concurrently pnpm:watch:*",
    "watch:tsc": "tsc --preserveWatchOutput --watch & tsc-alias --watch",
    "watch:templates": "pnpm build:templates --watch",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier -w ."
  },
  "dependencies": {
    "@halfdomelabs/core-generators": "workspace:*",
    "@halfdomelabs/sync": "workspace:*",
    "@prisma/internals": "4.16.2",
    "@types/eslint": "^8.44.1",
    "@types/resolve": "^1.20.2",
    "change-case": "^4.1.2",
    "indent-string": "5.0.0",
    "inflection": "2.0.1",
    "ramda": "^0.28.0",
    "resolve": "^1.22.2",
    "semver": "^7.5.4",
    "ts-morph": "18.0.0",
    "zod": "3.21.4"
  },
  "engines": {
    "node": "^18.0.0"
  },
  "volta": {
    "node": "18.17.1"
  },
  "type": "module"
}
