{
  "name": "@halfdomelabs/fastify-generators",
  "version": "0.2.0",
  "description": "Fastify generators for Baseplate",
  "main": "lib/index.js",
  "repository": "https://github.com/halfdomelabs/baseplate",
  "author": "Half Dome Labs LLC",
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "restricted",
    "registry": "https://npm.pkg.github.com"
  },
  "files": [
    "LICENSE",
    "generator.json",
    "lib/**/*"
  ],
  "devDependencies": {
    "@halfdomelabs/tools": "0.2.0",
    "@types/inflection": "^1.13.0",
    "@types/ramda": "^0.27.64",
    "@types/semver": "^7.3.9",
    "concurrently": "^7.0.0",
    "del": "^6.0.0",
    "tsc-alias": "^1.8.2",
    "typescript": "4.9.5"
  },
  "scripts": {
    "build": "concurrently yarn:build:*",
    "build:tsc": "rm -rf ./lib && tsc && tsc-alias",
    "build:templates": "gulp buildTemplates",
    "watch": "concurrently yarn:watch:*",
    "watch:tsc": "tsc --preserveWatchOutput --watch & tsc-alias --watch",
    "watch:templates": "gulp watchTemplates",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier -w .",
    "release": "yarn publish --non-interactive"
  },
  "dependencies": {
    "@halfdomelabs/core-generators": "0.2.0",
    "@halfdomelabs/sync": "0.2.0",
    "@prisma/internals": "4.0.0",
    "@types/eslint": "^8.2.2",
    "@types/resolve": "^1.20.1",
    "change-case": "^4.1.2",
    "gulp": "^4.0.2",
    "indent-string": "4.0.0",
    "inflection": "^1.13.2",
    "ramda": "^0.28.0",
    "resolve": "^1.21.0",
    "semver": "^7.3.5",
    "ts-morph": "^13.0.2",
    "zod": "^3.20.6"
  },
  "engines": {
    "node": "^16.0.0",
    "yarn": "^1.22.17"
  },
  "volta": {
    "node": "16.15.0",
    "yarn": "1.22.17"
  }
}
