{
  "name": "@halfdomelabs/fastify-generators",
  "version": "0.8.0",
  "description": "Fastify generators for Baseplate",
  "repository": "https://github.com/halfdomelabs/baseplate",
  "license": "SEE LICENSE IN LICENSE",
  "author": "Half Dome Labs LLC",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "LICENSE",
    "generator.json",
    "dist/**/*",
    "CHANGELOG"
  ],
  "scripts": {
    "build": "concurrently pnpm:build:*",
    "build:templates": "cpx \"src/generators/*/*/templates/**\" \"dist/generators\" --clean",
    "build:tsc": "tsc && tsc-alias",
    "clean": "rm -rf ./dist",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier -w .",
    "watch": "pnpm build && concurrently pnpm:watch:*",
    "watch:templates": "pnpm build:templates --watch",
    "watch:tsc": "tsc --preserveWatchOutput -w",
    "watch:tsc-alias": "tsc-alias -w"
  },
  "dependencies": {
    "@halfdomelabs/core-generators": "workspace:*",
    "@halfdomelabs/sync": "workspace:*",
    "@prisma/internals": "5.3.1",
    "@types/eslint": "^8.44.1",
    "@types/resolve": "^1.20.2",
    "change-case": "^4.1.2",
    "indent-string": "5.0.0",
    "inflection": "2.0.1",
    "ramda": "^0.28.0",
    "resolve": "^1.22.2",
    "semver": "^7.5.4",
    "ts-morph": "18.0.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@halfdomelabs/tools": "workspace:*",
    "@types/ramda": "^0.29.3",
    "@types/semver": "^7.5.0",
    "concurrently": "8.2.2",
    "cpx2": "^5.0.0",
    "tsc-alias": "1.8.8",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": "^20.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
