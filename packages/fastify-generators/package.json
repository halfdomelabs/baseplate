{
  "name": "@halfdomelabs/fastify-generators",
  "version": "0.13.1",
  "description": "Fastify generators for Baseplate",
  "homepage": "https://www.halfdomelabs.com/",
  "repository": {
    "type": "git",
    "url": "https://github.com/halfdomelabs/baseplate",
    "directory": "packages/fastify-generators"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": "Half Dome Labs LLC",
  "sideEffects": false,
  "type": "module",
  "imports": {
    "#src/*": "./dist/*"
  },
  "main": "dist/index.js",
  "files": [
    "LICENSE",
    "dist/**/*",
    "!dist/**/*.d.ts.map",
    "!dist/**/*.tsbuildinfo",
    "CHANGELOG"
  ],
  "scripts": {
    "build": "concurrently pnpm:build:*",
    "build:templates": "cpx \"src/generators/*/*/templates/**\" \"dist/generators\" --clean",
    "build:tsc": "tsc -p tsconfig.build.json",
    "clean": "rm -rf ./dist",
    "lint": "eslint .",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier -w .",
    "typecheck": "tsc --noEmit",
    "watch": "concurrently pnpm:watch:*",
    "watch:templates": "pnpm build:templates --watch",
    "watch:tsc": "tsc -p tsconfig.build.json --preserveWatchOutput -w"
  },
  "dependencies": {
    "@halfdomelabs/core-generators": "workspace:*",
    "@halfdomelabs/sync": "workspace:*",
    "@halfdomelabs/utils": "workspace:*",
    "@prisma/internals": "6.5.0",
    "change-case": "5.4.4",
    "es-toolkit": "1.31.0",
    "indent-string": "5.0.0",
    "inflection": "3.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@halfdomelabs/tools": "workspace:*",
    "@tailwindcss/forms": "0.5.9",
    "concurrently": "9.0.1",
    "cpx2": "7.0.1",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": "^22.0.0"
  },
  "volta": {
    "extends": "../../package.json"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
