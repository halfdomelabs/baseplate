{
  "name": "core/fastify-sentry",
  "templates": {
    "src/instrument.ts": {
      "name": "instrument",
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "generator": "@baseplate-dev/fastify-generators#core/fastify-sentry",
      "importMapProviders": {
        "configServiceImportsProvider": {
          "importName": "configServiceImportsProvider",
