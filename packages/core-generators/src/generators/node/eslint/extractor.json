{
  "name": "node/eslint",
  "templates": {
    "eslint.config.js": {
      "name": "eslint-config",
      "type": "ts",
      "generator": "@baseplate-dev/core-generators#node/eslint",
      "template": "eslint.config.js",
      "fileOptions": {
        "kind": "singleton",
        "generatorTemplatePath": "eslint.config.js"
      },
      "pathRootRelativePath": "{package-root}/eslint.config.mjs",
      "variables": {
        "TPL_DEFAULT_PROJECT_FILES": {},
        "TPL_DEV_DEPENDENCIES": {},
        "TPL_EXTRA_CONFIGS": {},
        "TPL_IGNORE_FILES": {}
      },
      "importMapProviders": {}
    }
  }
}
