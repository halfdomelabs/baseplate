{
  "name": "@halfdomelabs/core-generators",
  "version": "0.2.1",
  "description": "Core generators for Baseplate",
  "main": "dist/index.js",
  "repository": "https://github.com/halfdomelabs/baseplate",
  "author": "Half Dome Labs LLC",
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "LICENSE",
    "generator.json",
    "dist/**/*",
    "CHANGELOG"
  ],
  "devDependencies": {
    "@halfdomelabs/tools": "workspace:*",
    "@types/fs-extra": "11.0.1",
    "@types/is-core-module": "^2.2.0",
    "@types/lodash": "^4.14.191",
    "@types/ramda": "^0.27.64",
    "@types/semver": "^7.5.0",
    "concurrently": "^7.0.0",
    "del": "^6.0.0",
    "jest": "29.5.0",
    "ts-jest": "29.0.5",
    "tsc-alias": "1.8.6",
    "typescript": "4.9.5",
    "vitest": "0.32.2"
  },
  "scripts": {
    "build": "concurrently pnpm:build:*",
    "build:tsc": "tsc && tsc-alias",
    "build:templates": "gulp -f gulpfile.cjs buildTemplates",
    "watch": "concurrently pnpm:watch:*",
    "watch:tsc": "tsc --preserveWatchOutput --watch & tsc-alias --watch",
    "watch:templates": "gulp -f gulpfile.cjs watchTemplates",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier -w ."
  },
  "dependencies": {
    "@halfdomelabs/sync": "workspace:*",
    "@types/eslint": "^8.2.2",
    "@types/resolve": "^1.20.2",
    "fs-extra": "11.1.1",
    "gulp": "^4.0.2",
    "is-core-module": "^2.8.1",
    "lodash": "^4.17.21",
    "prettier": "2.8.4",
    "ramda": "^0.28.0",
    "resolve": "^1.22.2",
    "semver": "^7.5.1",
    "sort-keys": "^4.2.0",
    "ts-morph": "^13.0.3",
    "zod": "^3.20.6"
  },
  "engines": {
    "node": "^18.0.0"
  },
  "volta": {
    "node": "18.16.0"
  },
  "type": "module"
}
