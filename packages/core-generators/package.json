{
  "name": "@halfdomelabs/core-generators",
  "version": "0.6.1",
  "description": "Core generators for Baseplate",
  "main": "dist/index.js",
  "repository": "https://github.com/halfdomelabs/baseplate",
  "author": "Half Dome Labs LLC",
  "license": "SEE LICENSE IN LICENSE",
  "publishConfig": {
    "access": "restricted"
  },
  "files": [
    "LICENSE",
    "generator.json",
    "dist/**/*",
    "CHANGELOG"
  ],
  "devDependencies": {
    "@halfdomelabs/tools": "workspace:*",
    "@types/fs-extra": "11.0.1",
    "@types/is-core-module": "^2.2.0",
    "@types/lodash": "4.14.196",
    "@types/prettier": "^2.7.3",
    "@types/ramda": "^0.29.3",
    "@types/semver": "^7.5.0",
    "concurrently": "^8.2.0",
    "cpx2": "^5.0.0",
    "tsc-alias": "1.8.7",
    "typescript": "5.0.4",
    "vitest": "0.33.0"
  },
  "scripts": {
    "build": "concurrently pnpm:build:*",
    "build:tsc": "tsc && tsc-alias",
    "build:templates": "cpx \"src/generators/*/*/templates/**\" \"dist/generators\" --clean",
    "watch": "concurrently pnpm:watch:*",
    "watch:tsc": "tsc --preserveWatchOutput --watch & tsc-alias --watch",
    "watch:templates": "pnpm build:templates --watch",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx ./src",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "prettier:check": "prettier --check .",
    "prettier:format": "prettier -w ."
  },
  "dependencies": {
    "@halfdomelabs/sync": "workspace:*",
    "@types/eslint": "^8.44.1",
    "@types/resolve": "^1.20.2",
    "fs-extra": "11.1.1",
    "is-core-module": "2.12.1",
    "lodash": "^4.17.21",
    "prettier": "2.8.8",
    "ramda": "^0.28.0",
    "resolve": "^1.22.2",
    "semver": "^7.5.4",
    "sort-keys": "^5.0.0",
    "sort-package-json": "^2.5.1",
    "ts-morph": "18.0.0",
    "zod": "3.21.4"
  },
  "engines": {
    "node": "^18.0.0"
  },
  "volta": {
    "node": "18.17.1"
  },
  "type": "module"
}
