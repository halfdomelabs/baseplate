        },
        "reactSessionImportsProvider": {
          "importName": "reactSessionImportsProvider",
          "packagePathSpecifier": "@baseplate-dev/plugin-auth:src/auth/core/generators/react-session/generated/ts-import-providers.ts"
        }
      },
      "pathRootRelativePath": "{src-root}/hooks/use-log-out.ts",
