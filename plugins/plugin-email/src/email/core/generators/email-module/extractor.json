{
  "name": "email/core/email-module",
  "templates": {
    "emails-service": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "group": "main",
      "importMapProviders": {
        "configServiceImportsProvider": {
          "importName": "configServiceImportsProvider",
          "packagePathSpecifier": "@baseplate-dev/fastify-generators:src/generators/core/config-service/generated/ts-import-providers.ts"
        }
      },
      "pathRootRelativePath": "{module-root}/emails/services/emails.service.ts",
      "projectExports": {
        "sendRawEmail": { "isTypeOnly": false, "name": "sendRawEmail" }
      },
      "referencedGeneratorTemplates": ["emails-types", "send-email-queue"],
      "sourceFile": "module/emails/services/emails.service.ts",
      "variables": {}
    },
    "emails-types": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "group": "main",
      "importMapProviders": {
        "queuesImportsProvider": {
          "importName": "queuesImportsProvider",
          "packagePathSpecifier": "@baseplate-dev/plugin-queue:src/queue/core/generators/queues/generated/ts-import-providers.ts"
        }
      },
      "pathRootRelativePath": "{module-root}/emails/emails.types.ts",
      "projectExports": {
        "EmailAdapter": { "isTypeOnly": true, "name": "EmailAdapter" },
        "EmailAttachment": { "isTypeOnly": true, "name": "EmailAttachment" },
        "EmailRawOptions": { "isTypeOnly": true, "name": "EmailRawOptions" },
        "EmailSendOptions": { "isTypeOnly": true, "name": "EmailSendOptions" },
        "TransformedEmailMessage": {
          "isTypeOnly": true,
          "name": "TransformedEmailMessage"
        }
      },
      "sourceFile": "module/emails/emails.types.ts",
      "variables": {}
    },
    "send-email-queue": {
      "type": "ts",
      "fileOptions": { "kind": "singleton" },
      "group": "main",
      "importMapProviders": {
        "loggerServiceImportsProvider": {
          "importName": "loggerServiceImportsProvider",
          "packagePathSpecifier": "@baseplate-dev/fastify-generators:src/generators/core/logger-service/generated/ts-import-providers.ts"
        },
        "queueServiceImportsProvider": {
          "importName": "queueServiceImportsProvider",
          "packagePathSpecifier": "@baseplate-dev/plugin-queue:src/queue/providers/queue-service.provider.ts"
        }
      },
      "pathRootRelativePath": "{module-root}/emails/queues/send-email.queue.ts",
      "projectExports": {
        "sendEmailQueue": { "isTypeOnly": false, "name": "sendEmailQueue" }
      },
      "referencedGeneratorTemplates": ["emails-types"],
      "sourceFile": "module/emails/queues/send-email.queue.ts",
      "variables": { "TPL_EMAIL_ADAPTER": {} }
    }
  }
}
